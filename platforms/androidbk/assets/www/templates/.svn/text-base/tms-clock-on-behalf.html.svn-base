<script type="text/javascript" src="js/controllers/myLeave.js"></script>
<!--start Apply leave-->
<div class="form" >
  <div class="screen-submit">
    <h1 class="title-form background-ff6600">{{lang.mytms.clockOnBehalf.tt}}</h1>
    <div class="form-info scroll" ng-element-height="90">

      <ul class="list form-apply">
        <li ng-click="showPoupEmployee()">
